{
    "manifest_version":2,
    "name": "New Tab New Knowledge",
    "version": "1.0",

    "description": "Opens random wikipedia tab in the new tab",

    "permissions":[
        "tabs"
    ],
    "browser_action": {
    	"default_icon": "Icons/action_temp.svg",
	"default_title": "New Tab New Knowledge"
    },

    "chrome_url_overrides" : {
        "newtab": "knowledgetab.html"
    }
}
